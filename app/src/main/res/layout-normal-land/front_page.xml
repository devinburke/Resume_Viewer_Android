<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/AppTheme"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frontpagelayout"
    android:background="@drawable/lg"

 >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/welcome"
        android:textSize = "30sp"
        android:textStyle="bold"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true" />
    <Button
        android:id="@+id/bsq"
        android:layout_width="250dp"
        android:layout_height="75dp"
        android:textSize = "30sp"
        android:text="@string/sq"
        android:textStyle="bold"
        android:background="@drawable/custom_button"
        android:textColor="#000000"
        android:layout_marginTop="76dp"

        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="33dp"
        android:layout_marginStart="33dp" />
    <Button
        android:id="@+id/btq"
        android:layout_width="250dp"
        android:layout_height="75dp"
        android:textSize = "30sp"
        android:textColor="#000000"
        android:textStyle="bold"
        android:background="@drawable/custom_button"
        android:text= "@string/tq"
        android:layout_alignTop="@+id/bsq"
        android:layout_marginRight="25dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <Button
        android:id="@+id/other"
        android:layout_width="250dp"
        android:layout_height="75dp"
        android:textSize = "30sp"
        android:background="@drawable/custom_button"
        android:textColor="#000000"
        android:text= "@string/seeother"
        android:textStyle="bold"
        android:layout_marginTop="41dp"

        android:layout_below="@+id/bsq"
        android:layout_alignLeft="@+id/bsq"
        android:layout_alignStart="@+id/bsq" />
    <Button
        android:id="@+id/rate"
        android:layout_width="250dp"
        android:layout_height="75dp"
        android:textSize = "30sp"
        android:textColor="#000000"
        android:background="@drawable/custom_button"
        android:text= "@string/ra"
        android:textStyle="bold"
        android:layout_alignTop="@+id/other"
        android:layout_alignLeft="@+id/btq"
        android:layout_alignStart="@+id/btq" />

    <ImageView
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:id="@+id/imageFrame"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        />
    <ImageView
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:id="@+id/imageFrame2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        />


</RelativeLayout>
